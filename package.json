{
  "name": "androme",
  "version": "1.10.1",
  "description": "Converts and optimizes HTML pages with JavaScript into the various standard Android layouts in XML. iOS will be supported through Flutter integration. Compatible with Chrome and Safari.",
  "main": "dist/androme.min.js",
  "scripts": {
    "express": "node app.js",
    "lint": "tslint src/*.ts -t verbose",
    "dev": "tsc -w",
    "prod": "npm run clean && tslint -c tslint.json 'src/**/*.ts' && tsc --inlineSourceMap false && rollup -c rollup.config.js",
    "clean": "rimraf build/* && rimraf dist/* && rimraf temp/*"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/anpham6/androme.git"
  },
  "keywords": [
    "Android",
    "Flutter",
    "iOS",
    "Chrome",
    "Safari",
    "TypeScript",
    "Javascript",
    "HTML",
    "CSS",
    "Java",
    "Dart",
    "Kotlin",
    "Swift",
    "Android Studio",
    "Xcode",
    "iPhone",
    "Cordova",
    "PhoneGap",
    "Xamarin",
    "Xamarin Forms",
    "LinearLayout",
    "RelativeLayout",
    "ConstraintLayout",
    "GridLayout",
    "FrameLayout",
    "Flexbox",
    "Edge",
    "Firefox",
    "XML"
  ],
  "author": {
    "name": "An Pham",
    "email": "anpham6@gmail.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/anpham6/androme/issues"
  },
  "homepage": "https://github.com/anpham6/androme#readme",
  "dependencies": {
    "archiver": "^3.0.0",
    "body-parser": "^1.18.3",
    "express": "^4.16.3",
    "mkdirp": "^0.5.1",
    "request": "^2.88.0",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/preset-env": "^7.1.0",
    "rimraf": "^2.6.2",
    "rollup": "^0.66.4",
    "rollup-plugin-babel": "^4.0.3",
    "rollup-plugin-terser": "^3.0.0",
    "systemjs": "^0.21.5",
    "systemjs-plugin-babel": "0.0.25",
    "terser": "^3.9.3",
    "tslint": "^5.11.0",
    "typescript": "^3.1.1"
  }
}
