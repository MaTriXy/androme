{
  "name": "androme",
  "version": "1.7.32",
  "description": "Converts and optimizes HTML pages with JavaScript into the various standard Android layouts in XML. Resources are also auto-generated. iOS will be supported including a browser based UI designer.",
  "scripts": {
    "express": "node app.js",
    "dev": "tsc -w",
    "prod": "npm run clean && tslint -c tslint.json 'src/**/*.ts' && tsc --inlineSourceMap false && rollup -c rollup.config.js",
    "clean": "rimraf build/* && rimraf dist/* && rimraf temp/*"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/anpham6/androme.git"
  },
  "keywords": [
    "Android",
    "Android Studio",
    "iPhone",
    "iOS",
    "Xcode",
    "Chrome",
    "Mobile",
    "Phone",
    "Tablet",
    "XML",
    "TypeScript",
    "Javascript",
    "Kotlin",
    "Swift",
    "Xamarin",
    "Xamarin Forms",
    "ES6",
    "HTML5",
    "CSS3",
    "Flex",
    "Flexbox",
    "LinearLayout",
    "RelativeLayout",
    "ConstraintLayout",
    "GridLayout",
    "CoordinatorLayout",
    "TableLayout",
    "DrawerLayout",
    "Storyboard"
  ],
  "author": {
    "name": "An Pham",
    "email": "anpham6@gmail.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/anpham6/androme/issues"
  },
  "homepage": "https://github.com/anpham6/androme#readme",
  "dependencies": {
    "archiver": "^2.1.1",
    "body-parser": "^1.18.3",
    "express": "^4.16.3",
    "mkdirp": "^0.5.1",
    "request": "^2.87.0",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-preset-env": "^1.7.0",
    "nodemon": "^1.17.5",
    "rimraf": "^2.6.2",
    "rollup": "^0.59.4",
    "rollup-plugin-babel": "^3.0.7",
    "rollup-plugin-babel-minify": "^5.0.0",
    "rollup-plugin-buble": "^0.19.2",
    "systemjs": "^0.21.4",
    "systemjs-plugin-babel": "0.0.25",
